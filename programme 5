========================
FILE: imagemap.html
========================
<!DOCTYPE html>
<html>
<body>

    <h3>Click the sections below</h3>

    <img src="votingmap.jpg" usemap="#votemap" width="300" height="300">

    <map name="votemap">

        <area shape="rect" coords="0,0,150,150"
              href="home.html" target="rightframe">

        <area shape="rect" coords="150,0,300,150"
              href="candidates.html" target="rightframe">

        <area shape="rect" coords="0,150,300,300"
              href="contact.html" target="rightframe">

    </map>

</body>
</html>


